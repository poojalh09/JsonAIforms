{
  "title": "Clinical Trial Enrollment Form",
  "layout": "vertical",
  "fields": [
    {
      "name": "participantName",
      "label": "Participant Name",
      "type": "text",
      "placeholder": "Enter your full name",
      "required": true,
      "validation": {
        "required": true,
        "pattern": "^[a-zA-Z\\s]+$",
        "min": "3",
        "max": "100"
