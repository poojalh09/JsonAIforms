{
  "title": "Clinical Trial Enrollment Form",
  "layout": "vertical",
  "fields": [
    {
      "name": "patientName",
      "label": "Patient Full Name",
      "type": "text",
      "placeholder": "Enter full name",
      "required": true,
      "validation": {
        "required": true,
        "pattern": "^[a-zA-Z\\s]+$",
