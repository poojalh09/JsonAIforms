{
  "title": "Clinical Trial Enrollment Form",
  "layout": "vertical",
  "fields": [
    {
      "name": "patientName",
      "label": "Patient Name",
      "type": "text",
      "placeholder": "Enter full name",
      "required": true,
      "validation": {
        "required": true,
        "pattern": "^[a-zA-Z\\s]+$",
        "min": "3",
        "max": "100"
      }
    },
    {
