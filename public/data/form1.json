{
  "title": "User Profile Form",
  "layout": "grid-cols-2 gap-4",
  "fields": [
    { "name": "full_name", "label": "Full Name", "type": "text", "position": "col-span-2" },
    { "name": "email", "label": "Email Address", "type": "email","required": true, "position": "col-span-1" },
    { "name": "password", "label": "Password", "type": "password","required": true, "position": "col-span-1" },
    { "name": "bio", "label": "Short Bio", "type": "textarea", "position": "col-span-2" },
    { "name": "dob", "label": "Date of Birth ", "type": "date", "position": "col-span-1" },
    { "name": "gender", "label": "Gender", "type": "radio", "options": ["Male", "Female", "Other"], "position": "col-span-1" },
    { "name": "skills", "label": "Skills", "type": "checkbox","required": false, "options": ["JavaScript", "Python", "React", "Node.js"], "position": "col-span-2" },
    { "name": "country", "label": "Country", "type": "select", "options": ["USA", "Canada", "UK", "India"], "position": "col-span-1" },
    { "name": "language", "label": "Preferred Language","type": "combobox", "options": ["English", "Spanish", "French", "German", "Chinese"], "position": "col-span-1" },
    { "name": "newsletter", "label": "Subscribe to Newsletter ", "type": "switch", "position": "col-span-1" },
    { "name": "experience", "label": "Experience Level", "type": "slider", "position": "col-span-1" },
    { "name": "resume", "label": "Upload Resume", "type": "file", "position": "col-span-2" },
    { "name": "tags", "label": "Tags (Press Enter to Add)", "type": "tags", "position": "col-span-2" }
    
  ]
}